RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^ index.php [QSA,L]

RewriteRule ^views/(.*)$ - [F,L]
<FilesMatch "\.php$">
    Require all denied
</FilesMatch>
<Files "index.php">
    Require all granted
</Files>
<FilesMatch ".env">
    Require all denied
</FilesMatch>
<FilesMatch "migrate">
    Require all denied
</FilesMatch>
<FilesMatch "\.md$">
    Require all denied
</FilesMatch>
